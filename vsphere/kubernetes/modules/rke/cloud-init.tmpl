#cloud-config

hostname: ${host}

rancher:

  resize_device: /dev/sda
  
  network:
    interfaces:
      eth0:
        address: ${address}/${netmask}
        gateway: ${gateway}
        mtu: 1500
        dhcp: false
    dns:
      nameservers:
        - ${dns1}
        - ${dns2}
        
ssh_authorized_keys:
  - ssh-rsa 
  - ssh-rsa 
  - ssh-rsa 
  - ssh-rsa  
  
